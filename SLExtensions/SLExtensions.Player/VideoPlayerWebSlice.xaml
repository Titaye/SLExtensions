                                    <GradientStop Color="#FFFFFFFF" Offset="1"/>
                                </LinearGradientBrush>
                            </Path.Stroke>
                            <Path.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform ScaleX="0.2" ScaleY="0.2"/>
                                    <SkewTransform/>
                                    <RotateTransform/>
                                    <TranslateTransform/>
                                </TransformGroup>
                            </Path.RenderTransform>
                            <Path.Fill>
                                <RadialGradientBrush RadiusX="0.5" RadiusY="0.499998" Center="0.499999,0.500001" GradientOrigin="0.499999,0.500001">
                                    <RadialGradientBrush.RelativeTransform>
                                        <TransformGroup/>
                                    </RadialGradientBrush.RelativeTransform>
                                    <GradientStop Color="#00444444" Offset="0.431535"/>
                                    <GradientStop Color="#FF131313" Offset="0.875519"/>
                                    <GradientStop Color="#00444444" Offset="1"/>
                                </RadialGradientBrush>
                            </Path.Fill>
                        </Path>
                        <TextBlock Height="Auto" Width="62" TextWrapping="Wrap" FontSize="16" Text="{Binding Converter={StaticResource percentConverter}, ConverterParameter=0 -%, Path=BufferingProgress, Source={StaticResource controller}}" FontFamily="Arial" FontStyle="Normal" FontWeight="Normal" HorizontalAlignment="Center" VerticalAlignment="Center" x:Name="textBlock1" Visibility="Collapsed" TextAlignment="Center" Canvas.Left="0" Canvas.Top="22">
					<TextBlock.Foreground>
						<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
							<GradientStop Color="#FF000000" Offset="0.714"/>
							<GradientStop Color="#FFA0A0A0" Offset="0.241"/>
						</LinearGradientBrush>
					</TextBlock.Foreground>
                        </TextBlock>
                    </Canvas>
                </Grid>
                <Grid Height="20" x:Name="sliderFull" VerticalAlignment="Bottom" Visibility="Collapsed" Margin="2,0,5,-7">
                    <Slider Maximum="1" IsHitTestVisible="False" 
        		Value="{Binding Path=DownloadProgress, Source={StaticResource controller}}" x:Name="sliderLoading_Copy" Style="{StaticResource SliderLoading-}" RenderTransformOrigin="0.5,0.5" >
                        <Slider.RenderTransform>
                            <TransformGroup>
                                <ScaleTransform/>
                                <SkewTransform/>
                                <RotateTransform/>
                                <TranslateTransform/>
                            </TransformGroup>
                        </Slider.RenderTransform>
                    </Slider>

                    <slec:Slider  x:Name="sliderTimeline_Copy"
        		Maximum="{Binding Path=End, Converter={StaticResource timeSpanConverter}, Source={StaticResource controller}}"
                         Minimum="{Binding Path=Start, Converter={StaticResource timeSpanConverter}, Source={StaticResource controller}}"
        				 MoveValue="{Binding Converter={StaticResource timeSpanConverter}, Mode=TwoWay, Path=Position, Source={StaticResource controller}}" Foreground="#34FFFFFF" Style="{StaticResource SliderStyleProgress}" RenderTransformOrigin="0.5,0.5"  
                           >
                        <slec:Slider.RenderTransform>
                            <TransformGroup>
                                <ScaleTransform/>
                                <SkewTransform/>
                                <RotateTransform/>
                                <TranslateTransform/>
                            </TransformGroup>
                        </slec:Slider.RenderTransform>
                    </slec:Slider>



                </Grid>
                <ContentControl VerticalAlignment="Bottom" Height="150" mediaCore:MediaController.ContentPub="{StaticResource controller}" />

            </Grid>
        </Canvas>
        <TextBlock Text="{Binding Mode=TwoWay, Path=CurrentItem.Title, Source={StaticResource controller}}" x:Name="title"
					TextWrapping="Wrap"
					FontSize="18" Margin="5,0,5,2" VerticalAlignment="Bottom" RenderTransformOrigin="0.5,0.5" >
					<TextBlock.RenderTransform>
						<TransformGroup>
							<ScaleTransform ScaleY="1"/>
							<SkewTransform/>
							<RotateTransform/>
							<TranslateTransform/>
						</TransformGroup>
					</TextBlock.RenderTransform>
        </TextBlock>
    </Grid>
</UserControl>

